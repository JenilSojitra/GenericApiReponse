<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>GenericApiResponse</PackageId>
    <Version>1.0.0</Version>
    <Authors>Jenil Sojitra</Authors>
    <Company>Chandra Solution</Company>
    <Description>A generic API response wrapper for .NET applications.</Description>
    <PackageTags>API Response Wrapper Generic C#</PackageTags>
    <RepositoryUrl>https://github.com/yourrepo</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.3.0" />
  </ItemGroup>

</Project>
